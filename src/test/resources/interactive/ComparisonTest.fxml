<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXCheckbox?>
<?import io.github.palexdev.materialfx.controls.MFXFilterComboBox?>
<?import io.github.palexdev.materialfx.controls.MFXTextField?>
<?import io.github.palexdev.materialfx.controls.MFXTitledPane?>
<?import io.github.palexdev.mfxcore.controls.MFXIconWrapper?>
<?import io.github.palexdev.mfxresources.font.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.*?>
<StackPane id="rootPane" fx:id="rootPane" stylesheets="@ComparisonTest.css" xmlns="http://javafx.com/javafx/18"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="interactive.controller.ComparisonTestController">
   <VBox prefHeight="900.0" prefWidth="1440.0">
      <HBox fx:id="windowHeader" alignment="CENTER_RIGHT" spacing="10.0" styleClass="header">
         <padding>
            <Insets bottom="17.0" left="10.0" right="10.0" top="10.0"/>
         </padding>
         <Label fx:id="headerLabel" maxWidth="1.7976931348623157E308" text="Label" HBox.hgrow="ALWAYS"/>
         <MFXFontIcon id="minimizeIcon" onMouseClicked="#minimize" styleClass="headerIcon"/>
         <MFXFontIcon id="maximizeIcon" onMouseClicked="#maximize" styleClass="headerIcon"/>
         <MFXFontIcon id="closeIcon" description="" onMouseClicked="#close" styleClass="headerIcon"/>
      </HBox>
      <HBox id="actionsPane" alignment="CENTER" minHeight="60.0" spacing="15.0">
         <padding>
            <Insets bottom="10.0" top="10.0"/>
         </padding>
         <AnchorPane maxWidth="520.0" HBox.hgrow="ALWAYS">
            <Label fx:id="selectionLabel" layoutX="73.0" layoutY="12.0" text="Label"
                   AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="10.0"/>
         </AnchorPane>
         <Label text="Actions"/>
         <MFXFilterComboBox id="actionsBox" fx:id="actionsBox" floatMode="DISABLED" scrollOnOpen="true"/>
         <MFXIconWrapper id="runIcon" fx:id="runIcon" onMouseClicked="#runAction" size="27.0">
            <icon>
               <MFXFontIcon description="mfx-caret-right" size="24.0"/>
            </icon>
            <HBox.margin>
               <Insets left="5.0"/>
            </HBox.margin>
         </MFXIconWrapper>
         <MFXIconWrapper id="switchIcon" fx:id="switchIcon" onMouseClicked="#switchTarget" size="27.0">
            <icon>
               <MFXFontIcon description="mfx-sync" size="15.0"/>
            </icon>
            <HBox.margin>
               <Insets left="5.0"/>
            </HBox.margin>
         </MFXIconWrapper>
         <AnchorPane maxWidth="520.0" HBox.hgrow="ALWAYS"/>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <StackPane fx:id="leftPane" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                    minWidth="-Infinity" prefHeight="760.0" prefWidth="715.0" HBox.hgrow="ALWAYS">
            <HBox.margin>
               <Insets right="2.5"/>
            </HBox.margin>
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
         </StackPane>
         <Separator orientation="VERTICAL" prefHeight="200.0"/>
         <StackPane fx:id="rightPane" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                    minWidth="-Infinity" prefHeight="760.0" prefWidth="715.0" HBox.hgrow="ALWAYS">
            <HBox.margin>
               <Insets/>
            </HBox.margin>
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
         </StackPane>
      </HBox>
      <MFXTitledPane id="footer" title="Settings">
         <content>
            <AnchorPane minHeight="40.0">
               <padding>
                  <Insets bottom="5.0" top="5.0"/>
               </padding>
               <MFXCheckbox fx:id="ssCheck" gap="10.0" text="Smooth Scroll" AnchorPane.leftAnchor="20.0"
                            AnchorPane.topAnchor="10.0"/>
               <MFXCheckbox fx:id="modeCheck" text="Run on both" AnchorPane.leftAnchor="180.0"
                            AnchorPane.topAnchor="10.0"/>
               <MFXCheckbox fx:id="animCheck" text="Enable background animations" selected="true"
                            AnchorPane.leftAnchor="340.0" AnchorPane.topAnchor="10.0"/>
               <MFXTextField fx:id="durationField" floatingText="Background animation duration" layoutX="22.0"
                             layoutY="57.0" measureUnit="ms" prefHeight="38.0" prefWidth="250.0" text="500"
                             AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="50.0"/>
            </AnchorPane>
         </content>
      </MFXTitledPane>
   </VBox>
</StackPane>
